{
  "configs": [
    "/runtime.json",
    "/config.json",
    "/lang/lang.ru.json"
  ],
  "class": "col-sm-offset-1 col-sm-10 col-md-offset-2 col-md-8 col-lg-offset-3 col-lg-6",
  "content": [
    {
      "type": "h5",
      "title": "{{devname}}",
      "class": "alert-default"
    },
    {
      "type": "link",
      "title": "{{LS_main_page}}",
      "action": "/",
      "class": "btn btn-block btn-default"
    },
    {
      "type": "hr"
    },
    {
      "type": "h3",
      "title": "{{LS_i2c_scanner}}"
    },
    {
      "type": "h4",
      "title": "{{i2c}}"
    },
    {
      "type": "link",
      "title": "{{LS_scan}}",
      "action": "/set?i2c",
      "style": "margin:5% 20% 5%;width:60%",
      "class": "btn btn-block btn-default"
    },
    {
      "type": "hr"
    },
    {
      "type": "h3",
      "title": "{{LS_onewire_scanner}}"
    },
    {
      "type": "table",
      "state": "onewire.json",
      "style": "margin:0% 10% 0% 10%;width:80%",
      "title": {
        "num": "{{LS_num}}",
        "addr": "{{LS_address}}",
        "url": "{{LS_family}}"
      }
    },
    {
      "type": "link",
      "title": "{{LS_scan}}",
      "action": "/set?oneWire",
      "style": "margin:5% 20% 5%;width:60%",
      "class": "btn btn-block btn-default"
    },
    {
      "type": "hr"
    },
    {
      "type": "h3",
      "title": "{{LS_sensor_logs}}"
    },
    {
      "type": "table",
      "state": "logs.json",
      "style": "margin:0% 10% 0% 10%;width:80%",
      "title": {
        "name": "{{LS_name}}",
        "entries": "{{LS_entries}}",
        "size": "{{LS_size}}"
      }
    },
    {
      "type": "link",
      "title": "{{LS_clear}}",
      "action": "/set?clear_log",
      "style": "margin:5% 20% 5%;width:60%",
      "class": "btn btn-block btn-default"
    },
    {
      "type": "hr"
    },
    {
      "type": "h3",
      "title": "{{LS_available_version}}"
    },
    {
      "type": "h3",
      "title": "{{last_version}}"
    },
    {
      "type": "button",
      "title": "{{LS_update_firmware}}",
      "action": "/check",
      "response": "[[my-block]]",
      "style": "margin:5% 20% 5%;width:60%",
      "class": "btn btn-block btn-default"
    },
    {
      "type": "h3",
      "name": "my-block",
      "style": "position:fixed;top:50%;left:50%;width:20%;margin-left:-10%;text-align:center;",
      "class": "hidden"
    }
  ]
}