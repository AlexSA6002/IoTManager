{
  "configs": [
    "/config.json",
    "/lang/lang.ru.json"
  ],
  "class": "col-sm-offset-1 col-sm-10 col-md-offset-2 col-md-8 col-lg-offset-3 col-lg-6",
  "content": [
    {
      "type": "h5",
      "title": "{{devname}}",
      "class": "alert-default"
    },
    {
      "type": "link",
      "title": "{{LS_main_page}}",
      "action": "/",
      "class": "btn btn-block btn-center"
    },
    {
      "type": "hr"
    },
    {
      "type": "h3",
      "title": "{{LS_wifi_sta}}"
    },
    {
      "type": "h4",
      "title": "{{LS_SSID}}",
      "style": "width:40%;float:left;"
    },
    {
      "type": "input",
      "title": "",
      "name": "routerssid-arg",
      "style": "width:60%;float:right;",
      "state": "{{routerssid}}",
      "pattern": ".{1,31}"
    },
    {
      "type": "h4",
      "title": "{{LS_password}}",
      "style": "width:40%;float:left;"
    },
    {
      "type": "password",
      "title": "",
      "name": "routerpass-arg",
      "style": "width:60%;float:right;",
      "state": "{{routerpass}}",
      "pattern": ".{8,31}"
    },
    {
      "type": "button",
      "title": "{{LS_save}}",
      "class": "btn btn-block btn-center",
      "action": "set?routerssid=[[routerssid-arg]]&routerpass=[[routerpass-arg]]"
    },
    {
      "type": "hr"
    },
    {
      "type": "h4",
      "style": "width:30%;float:left;",
      "title": "{{LS_device_name}}"
    },
    {
      "type": "input",
      "title": "{{LS_device_name}}",
      "name": "hostname-arg",
      "state": "{{hostname}}",
      "style": "width:70%;float:right;",
      "pattern": "[A-Za-z0-9,_]{6,12}"
    },
    {
      "type": "button",
      "title": "{{LS_save}}",
      "action": "set?hostname=[[hostname-arg]]",
      "class": "btn btn-block btn-center"
    },
    {
      "type": "hr"
    },
    {
      "type": "checkbox",
      "name": "blink",
      "title": "{{LS_use_status_led}}",
      "action": "/set?blink=[[blink]]",
      "state": "{{blink}}"
    },
    {
      "type": "text",
      "title": "<div style='margin-top:10px;margin-bottom:10px;'><font color='black'><p style='border: 1px solid #DCDCDC; border-radius: 3px; background-color: #F5F5F5; padding: 10px;'>{{LS_led_status_note}}</p></font></div>"
    },
    {
      "type": "hr"
    },
    {
      "type": "h3",
      "title": "{{LS_wifi_ap}}"
    },
    {
      "type": "h4",
      "title": "{{LS_SSID}}"
    },
    {
      "type": "input",
      "title": "",
      "name": "apssid-arg",
      "state": "{{apssid}}",
      "pattern": ".{1,31}"
    },
    {
      "type": "h4",
      "title": "{{LS_password}}"
    },
    {
      "type": "password",
      "title": "",
      "name": "appass-arg",
      "state": "{{appass}}",
      "pattern": ".{8,31}"
    },
    {
      "type": "button",
      "title": "{{LS_save}}",
      "action": "set?apssid=[[apssid-arg]]&appass=[[appass-arg]]",
      "class": "btn btn-block btn-center"
    },
    {
      "type": "hr"
    },
    {
      "type": "h3",
      "title": "{{LS_web_credentials}}"
    },
    {
      "type": "h4",
      "title": "{{LS_login}}"
    },
    {
      "type": "input",
      "title": "",
      "name": "weblogin-arg",
      "state": "{{weblogin}}",
      "pattern": ".{1,31}"
    },
    {
      "type": "h4",
      "title": "{{LS_password}}"
    },
    {
      "type": "password",
      "title": "",
      "name": "webpass-arg",
      "state": "{{webpass}}",
      "pattern": ".{1,31}"
    },
    {
      "type": "button",
      "title": "{{LS_save}}",
      "action": "set?weblogin=[[weblogin-arg]]&webpass=[[webpass-arg]]",
      "class": "btn btn-block btn-center"
    },
    {
      "type": "hr"
    },
    {
      "type": "h3",
      "title": "{{LS_clock}}"
    },
    {
      "type": "h4",
      "title": "{{LS_timezone}}"
    },
    {
      "type": "input",
      "title": "",
      "name": "timezone-arg",
      "state": "{{timezone}}",
      "pattern": ".{1,20}"
    },
    {
      "type": "h4",
      "title": "{{LS_ntp}}"
    },
    {
      "type": "input",
      "title": "",
      "name": "ntp-arg",
      "state": "{{ntp}}"
    },
    {
      "type": "button",
      "title": "{{LS_save}}",
      "action": "set?timezone=[[timezone-arg]]&ntp=[[ntp-arg]]",
      "class": "btn btn-block btn-center"
    },
    {
      "type": "hr"
    },
    {
      "type": "h3",
      "title": "{{LS_update}}"
    },
    {
      "type": "h4",
      "title": "{{LS_update_url}}",
      "style": "width:30%;float:left;"
    },
    {
      "type": "input",
      "title": "",
      "name": "updateurl-arg",
      "style": "width:70%;float:right",
      "state": "{{updateurl}}"
    },
    {
      "type": "button",
      "title": "{{LS_save}}",
      "action": "set?updateurl=[[updateurl-arg]]",
      "class": "btn btn-block btn-center"
    },
    {
      "type": "hr"
    }
  ]
}