{
  "configs": [
    "/config.json",
    "/lang/lang.ru.json"
  ],
  "class": "col-sm-offset-1 col-sm-10 col-md-offset-2 col-md-8 col-lg-offset-3 col-lg-6",
  "reload": "10000",
  "content": [
    {
      "type": "h5",
      "title": "{{devname}}",
      "class": "alert-default"
    },
    {
      "type": "link",
      "title": "{{LS_mainPage}}",
      "action": "/",
      "class": "btn btn-block btn-default"
    },
    {
      "type": "checkbox",
      "name": "udponoff",
      "title": "{{LS_broadcastEnabled}}",
      "action": "/set?udponoff=[[udponoff]]",
      "state": "{{udpon off}}",
      "design": "two"
    },
    {
      "type": "h4",
      "title": "{{SetUDPNameOfDev}}",
      "style": "width:60%;float:left;"
    },
    {
      "type": "input",
      "title": "",
      "name": "devname-arg",
      "state": "{{devname}}",
      "pattern": "[A-Za-z0-9,_]{6,12}",
      "style": "width:40%;float:right"
    },
    {
      "type": "h4",
      "title": "{{LS_broadcastInterval}}",
      "style": "width:60%;float:left;"
    },
    {
      "type": "input",
      "title": "",
      "name": "udptime-arg",
      "state": "{{udptime}}",
      "pattern": "[0-9]{1,5}",
      "style": "width:40%;float:right;"
    },
    {
      "type": "button",
      "title": "{{LS_save}}",
      "action": "/set?devname=[[devname-arg]]&udptime=[[udptime-arg]]",
      "class": "btn btn-block btn-default"
    },
    {
      "type": "hr"
    },
    {
      "type": "h4",
      "title": "{{SetUDPList}}"
    },
    {
      "type": "table",
      "state": "device.json",
      "title": {
        "id": "ID ",
        "name": "Имя устройства",
        "url": "Адрес",
        "lastseen": "Время"
      }
    },
    {
      "type": "hr"
    },
    {
      "type": "link",
      "title": "{{LS_refresh}}",
      "action": "/?set.udp",
      "class": "btn btn-block btn-default"
    }
  ]
}