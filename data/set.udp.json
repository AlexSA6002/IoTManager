{
  "configs": [
    "/config.json",
    "/lang/lang.ru.json"
  ],
  "class": "col-sm-offset-1 col-sm-10 col-md-offset-2 col-md-8 col-lg-offset-3 col-lg-6",
  "content": [
    {
      "type": "h5",
      "title": "{{devname}}",
      "class": "alert-default"
    },
    {
      "type": "link",
      "title": "{{LS_main_page}}",
      "action": "/",
      "class": "btn btn-block btn-default"
    },
    {
      "type": "checkbox",
      "name": "udponoff",
      "title": "{{LS_broadcast_enabled}}",
      "action": "/set?udponoff=[[udponoff]]",
      "state": "{{udponoff}}",
      "design": "two"
    },
    {
      "type": "h4",
      "title": "{{LS_device_name}}",
      "style": "width:60%;float:left;"
    },
    {
      "type": "input",
      "title": "",
      "name": "devname-arg",
      "state": "{{devname}}",
      "pattern": "[A-Za-z0-9,_]{6,12}",
      "style": "width:40%;float:right"
    },
    {
      "type": "h4",
      "title": "{{LS_broadcast_interval}}",
      "style": "width:60%;float:left;"
    },
    {
      "type": "input",
      "title": "",
      "name": "udptime-arg",
      "state": "{{udptime}}",
      "pattern": "[0-9]{1,5}",
      "style": "width:40%;float:right;"
    },
    {
      "type": "button",
      "title": "{{LS_save}}",
      "action": "/set?devname=[[devname-arg]]&udptime=[[udptime-arg]]",
      "class": "btn btn-block btn-default"
    },
    {
      "type": "hr"
      
    },
    {
      "type": "h4",
      "title": "{{LS_known_devices}}"
    },
    {
      "type": "table",
      "state": "device.json",
      "title": {
        "id": "{{LS_device_id}}",
        "name": "{{LS_device_name}}",
        "url": "{{LS_address}}",
        "lastseen": "{{LS_time}}"
      }
    },
    {
      "type": "hr"
    },
    {
      "type": "link",
      "title": "{{LS_refresh}}",
      "action": "/?set.udp",
      "class": "btn btn-block btn-default"
    },
    {
      "type": "button",
      "title": "{{LS_share_mqtt_settings}}",
      "action": "set?share_mqtt",
      "class": "btn btn-block btn-default"
    }
  ]
}